{
  "name": "@hv-apps/data-visualization",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "conc -k --restart-tries 2 --restart-after 500 \"cd dpn && ./data-plane-engine\" \"cd dpn && ./microapp\" \"npm:dev-vite\"",
    "dev-vite": "vite",
    "preview": "conc -k --restart-tries 2 --restart-after 500 \"cd dpn && ./data-plane-engine\" \"cd dpn && ./microapp\" \"npm:preview-vite\"",
    "preview-vite": "vite preview",
    "build": "tsc && vite build",
    "check": "npm-run-all --parallel check:*",
    "check:types": "tsc --noEmit",
    "check:lint": "eslint --color --cache src",
    "check:format": "prettier --check .",
    "lint": "eslint --color --cache src --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@emotion/css": "^11.10.6",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@hitachivantara/app-shell": "^0.14.1",
    "@hitachivantara/uikit-react-core": "^5.5.0",
    "@hitachivantara/uikit-react-icons": "^5.1.3",
    "@hitachivantara/uikit-react-viz": "^5.1.1",
    "@mui/material": "^5.12.2",
    "apache-arrow": "^11.0.0",
    "arquero": "^5.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.10.6",
    "@hitachivantara/app-shell-vite-plugin": "^0.7.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitejs/plugin-react": "^3.1.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.39.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-tsconfig-paths": "^4.0.5"
  }
}
