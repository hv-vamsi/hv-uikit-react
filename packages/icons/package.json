{
  "name": "@hitachivantara/uikit-react-icons",
  "version": "5.7.2",
  "private": false,
  "description": "NEXT Design System icons packaged as a set of React components.",
  "author": "Hitachi Vantara UI Kit Team",
  "homepage": "https://github.com/lumada-design/hv-uikit-react",
  "keywords": [
    "hitachi-vantara",
    "design-system",
    "ui-kit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lumada-design/hv-uikit-react.git"
  },
  "bugs": {
    "url": "https://github.com/lumada-design/hv-uikit-react/issues"
  },
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "run-s clean convert",
    "build": "npm run prebuild && vite build",
    "postbuild": "npx cpy \"sprites\" \"dist\"",
    "optimize": "svgo -r -f assets assets",
    "preconvert": "npx rimraf bin",
    "convert": "run-s convert:*",
    "convert:svg": "tsx src/svgToReact.ts dir --input assets --output bin --force",
    "convert:sprites": "tsx src/svgToSprite.ts dir --input assets --output sprites --force",
    "convert:copy": "npx cpy \"lib/*\" \"bin\"",
    "clean": "npx rimraf dist bin sprites",
    "prepare": "npm run convert"
  },
  "peerDependencies": {
    "@emotion/styled": "^11.10.6",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@hitachivantara/uikit-styles": "workspace:*"
  },
  "devDependencies": {
    "@hitachivantara/eslint-config": "workspace:*",
    "@hitachivantara/tsconfig": "workspace:*",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "by-node-env": "^2.0.1",
    "cpy-cli": "^4.2.0",
    "html-to-react": "^1.6.0",
    "jsdom-no-contextify": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "recursive-readdir": "^2.2.3",
    "svgo": "^3.0.2",
    "tsx": "^3.12.7",
    "vite": "^4.3.9",
    "yargs": "^17.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
