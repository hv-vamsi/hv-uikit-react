{
  "name": "@hitachivantara/uikit-react-lab",
  "version": "5.14.0",
  "private": false,
  "description": "Contributed React components for the NEXT UI Kit.",
  "author": "Hitachi Vantara UI Kit Team",
  "homepage": "https://github.com/lumada-design/hv-uikit-react",
  "keywords": [
    "hitachi-vantara",
    "design-system",
    "ui-kit",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lumada-design/hv-uikit-react.git"
  },
  "bugs": {
    "url": "https://github.com/lumada-design/hv-uikit-react/issues"
  },
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && vite build",
    "test": "vitest run",
    "test:update": "vitest -u",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "clean": "npx rimraf dist",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.12.2",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@emotion/css": "^11.11.0",
    "@hitachivantara/uikit-react-core": "workspace:*",
    "@hitachivantara/uikit-react-icons": "workspace:*",
    "@hitachivantara/uikit-styles": "workspace:*",
    "lodash": "^4.17.21",
    "reactflow": "^11.10.1",
    "uid": "^2.0.2",
    "usehooks-ts": "^2.9.1",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@hitachivantara/tsconfig": "workspace:*",
    "@storybook/react": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "vite": "^4.3.9",
    "vitest": "^0.34.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
