{
  "name": "@hitachivantara/uikit-uno-preset",
  "version": "0.2.0",
  "private": false,
  "description": "UI Kit UnoCSS preset with the NEXT theme.",
  "keywords": [
    "hitachi-vantara",
    "design-system",
    "ui-kit",
    "react",
    "typescript"
  ],
  "homepage": "https://github.com/lumada-design/hv-uikit-react",
  "bugs": {
    "url": "https://github.com/lumada-design/hv-uikit-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lumada-design/hv-uikit-react.git"
  },
  "license": "Apache-2.0",
  "author": "Hitachi Vantara UI Kit Team",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js"
    }
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && vite build",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hitachivantara/uikit-styles": "^5.16.4",
    "@unocss/preset-rem-to-px": "^0.58.0",
    "unocss-preset-theme": "^0.12.0"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "vite": "^4.5.1"
  },
  "peerDependencies": {
    "@unocss/core": ">= 0.57.7",
    "@unocss/preset-uno": ">= 0.57.7"
